{
  "name": "pn-platform",
  "version": "1.0.5",
  "py-version": "1.0.3",
  "description": "專案",
  "scripts": {
    "lint": "tslint",
    "test": "jest",
    "dev": "nodemon",
    "dev:node": "node dist/server/server.bundle.js -p -w",
    "start": "NODE_ENV=production node dist/server/server.bundle.js",
    "storybook": "start-storybook",
    "dev:build": "grunt build:client && grunt build:server",
    "build": "grunt build:client --production && grunt build:server --production"
  },
  "nodemonConfig": {
    "watch": [
      "server",
      "pyscript"
    ],
    "inspect": true,
    "ext": "ts",
    "ignore": [
      "spec/**/*.spec.ts"
    ],
    "exec": "ts-node ./server/index.ts -p -w"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            2688
          ]
        }
      }
    }
  },
  "prettier": {
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always",
    "printWidth": 90
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier",
      "react"
    ],
    "rules": {
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/triple-slash-reference": 0,
      "require-atomic-updates": 0,
      "react/jsx-max-props-per-line": [
        1,
        {
          "maximum": 1,
          "when": "multiline"
        }
      ],
      "max-len": [
        1,
        90
      ],
      "prettier/prettier": "error"
    },
    "extends": [
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:import/typescript",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "*.js",
          "*.jsx"
        ],
        "rules": {
          "@typescript-eslint/recommended": "off",
          "@typescript-eslint/eslint-recommended": "off"
        }
      }
    ]
  },
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": ">=7.7.0",
    "@babel/plugin-proposal-object-rest-spread": ">=7.7.0",
    "@babel/plugin-syntax-dynamic-import": ">=7.7.0",
    "@babel/plugin-transform-runtime": ">=7.7.0",
    "@babel/preset-env": "7.7.0",
    "@babel/preset-react": "7.7.0",
    "@babel/preset-typescript": ">=7.7.4",
    "@babel/runtime": ">=7.4.3",
    "amqplib": "0.5.3",
    "axios": "0.18.1",
    "body-parser": "1.18.3",
    "commander": "3.0.0",
    "compression": "1.7.3",
    "copy-webpack-plugin": "5.0.4",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "express-fileupload": "1.1.5",
    "express-session": "1.15.6",
    "file-loader": "3.0.1",
    "helmet": "3.15.0",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "mongoose": "5.7.5",
    "morgan": "1.9.1",
    "nunjucks": "3.1.7",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "5.0.1",
    "react-hot-loader": "4.6.5",
    "saslprep": "^1.0.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "4.28.3",
    "webpack-dev-middleware": "3.5.1",
    "webpack-dev-server": "3.1.14",
    "webpack-hot-middleware": "2.24.3",
    "webpack-node-externals": "1.7.2",
    "ws": "7.0.0"
  },
  "devDependencies": {
    "@storybook/react": "5.0.3",
    "@types/amqplib": "0.5.12",
    "@types/compression": "0.0.36",
    "@types/dotenv": "6.1.0",
    "@types/express": "4.16.0",
    "@types/express-fileupload": "1.1.0",
    "@types/helmet": "0.0.42",
    "@types/jest": "23.3.12",
    "@types/lodash": "4.14.136",
    "@types/mongodb": "3.1.27",
